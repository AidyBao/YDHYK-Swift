import UIKit

class ZXAddTimeCell: UICollectionViewCell {
    
    static let ZXAddTimeCellID: String = "ZXAddTimeCell"

    override func awakeFromNib() {
        super.awakeFromNib()
        
        self.contentView.layer.cornerRadius = 16.0
        self.contentView.layer.masksToBounds = true
        
        self.contentView.layer.shadowOffset = CGSize.init(width: 0.0, height: 1.0)
        self.contentView.layer.shadowRadius = 16.0
        self.contentView.layer.shadowColor = UIColor.black.cgColor
        self.contentView.layer.shadowOpacity = 0.5
        
    }
}
